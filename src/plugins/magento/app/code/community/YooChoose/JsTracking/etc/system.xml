<?xml version="1.0"?>

<config>
    <sections>
        <yoochoose translate="label">
            <label>
                <![CDATA[<img width="16" height="16" src="data:image/png;base64,"/> Yoochoose JS-Tracking]]>
            </label>
            <tab>service</tab>
            <frontend_type>text</frontend_type>
            <sort_order>1000</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <yoochoose_script translate="label">
                    <label>Tracking Script</label>
                    <sort_order>5</sort_order>
                    <frontend_type>text</frontend_type>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <url translate="label">
                            <label>Script URL</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>50</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <validate>validate-url</validate>
                        </url>
                    </fields>
                </yoochoose_script>
                <yoochoose_product translate="label">
                    <label>Product Options</label>
                    <sort_order>6</sort_order>
                    <frontend_type>text</frontend_type>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <itemtypeid translate="label">
                            <label>Item type</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>50</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </itemtypeid>
                        <language translate="label">
                            <label>Language</label>
                            <frontend_type>select</frontend_type>
                            <source_model>yoochoose_jstracking/system_config_source_languages</source_model>
                            <sort_order>60</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </language>
                    </fields>
                </yoochoose_product>
            </groups>
        </yoochoose>
    </sections>
</config>